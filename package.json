{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate && prisma migrate deploy",
    "tsc": "tsc --noEmit",
    "db:migrate:dev": "dotenv -e .env -- prisma migrate dev",
    "db:studio:dev": "dotenv -e .env -- prisma studio",
    "db:migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
    "db:studio:prod": "dotenv -e .env.prod -- prisma studio"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "16.0.7",
    "next-auth": "5.0.0-beta.29",
    "next-intl": "^4.5.8",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.5.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prettier": "^3.6.2",
    "prisma": "^7.1.0",
    "sass": "^1.93.0",
    "typescript": "^5"
  }
}